{
    "name": "muffin/hits",
    "description": "Hits (view) counter for CakePHP 3 ORM.",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "counter",
        "view count"
    ],
    "homepage": "https://github.com/usemuffin/hits",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/hits/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/hits/issues",
        "source": "https://github.com/usemuffin/hits"
    },
    "require": {
        "cakephp/orm": "3.*"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.0",
        "cakephp/cakephp-codesniffer": "2.*",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Hits\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Hits\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
